@{
    ViewData["Title"] = "List";
}

@using Shop.Models

@model IEnumerable<Chair>

<ul>
    @{ 
        foreach (var item in Model)
        {
            <li> 
                Name: @item.Name 
                Desc: @item.Desc
                Category: @item.Category.Name
                Price: @item.Price
            </li>
        }
    }
</ul>