@{
    ViewData["Title"] = "List";
}

@using Shop.Models

@model IEnumerable<Chair>

<ul>
    @{ 
        foreach (var item in Model)
        {
        <li>
            <p>Name: @item.Name </p>
            <p>Desc: @item.Desc</p>
            <p>Category: @item.Category.Name</p>
            <p>Price: @item.Price</p>
            <p>IMG: @item.Img</p>
            <p></p>

        </li>
        }
    }
</ul>